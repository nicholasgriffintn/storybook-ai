{
  "name": "storybook-ai",
  "version": "0.1.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./preview": {
      "types": "./dist/index.d.ts",
      "import": "./dist/preview.js",
      "require": "./dist/preview.cjs"
    },
    "./preset": "./dist/preset.cjs",
    "./manager": "./dist/manager.js",
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "bundler": {
    "exportEntries": [
      "src/index.ts"
    ],
    "managerEntries": [
      "src/manager.ts"
    ],
    "previewEntries": [
      "src/preview.ts"
    ],
    "nodeEntries": [
      "src/preset.ts"
    ]
  },
  "storybook": {
    "displayName": "Storybook AI"
  },
  "scripts": {
    "build": "tsup --config ./tsup.config.ts"
  },
  "devDependencies": {
    "@storybook/components": "8.0.2",
    "@storybook/core-events": "8.0.2",
    "@storybook/icons": "^1.2.9",
    "@storybook/manager": "^8.0.5",
    "@storybook/manager-api": "^8.0.2",
    "@storybook/preview": "^8.0.5",
    "@storybook/preview-api": "^8.0.5",
    "@storybook/theming": "^8.0.2",
    "@storybook/types": "^8.0.5",
    "tsup": "^8.0.0"
  },
  "dependencies": {
    "@langchain/core": "^0.1.54",
    "@langchain/openai": "^0.0.26"
  }
}